<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<!-- tag(또는 node)의 순서에 주의 -->

	<!-- 전달인자로 null 줄 수 있음 -->
    <settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>

	<!-- mapper에서 사용할 타입이 길어서 짧게 별칭 지정 -->
	<typeAliases>
		<typeAlias type="human.class1.ajax.pmdto.Product_ManagementDTO" alias="pmDTO" />
		<typeAlias type="human.class1.ajax.pmdto.Product_Stock_ManagementDTO" alias="pm_stockDTO" />
		<typeAlias type="human.class1.ajax.pl_spDTO.Plan_ManagementDTO" alias="plDTO" />
		<typeAlias type="human.class1.ajax.pl_spDTO.Specification_ManagementDTO" alias="spDTO" />
		<typeAlias type="human.class1.ajax.shDTO.Shipping_ManagementDTO" alias="shDTO" />
		<typeAlias type="human.class1.ajax.PTDTO.Part_ManagementDTO" alias="ptDTO" />
		<typeAlias type="human.class1.ajax.PTDTO.Part_Stock_ManagementDTO" alias="pt_stockDTO" />
		<typeAlias type="human.class1.ajax.poDTO.Purchase_Order_ManagementDTO" alias="poDTO" />
		<typeAlias type="human.class1.ajax.stDTO.Stocking_ManagementDTO" alias="stDTO" />
		<typeAlias type="human.class1.ajax.bomDTO.Bom_ManagementDTO" alias="bomDTO" />
		<typeAlias type="human.class1.ajax.FlowDTO.FlowDTO" alias="flDTO" />
		<typeAlias type="human.class1.ajax.logindto.LoginDTO" alias="loginDTO"/>
		<typeAlias type="human.class1.ajax.boarddto.BoardDTO" alias="boardDTO"/>
		<typeAlias type="human.class1.ajax.boarddto.CommentDTO" alias="commentDTO"/>
	</typeAliases>

</configuration>